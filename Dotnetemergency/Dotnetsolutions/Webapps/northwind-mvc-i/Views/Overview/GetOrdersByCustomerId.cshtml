@model IEnumerable<northwind_app.Library.Models.Orders>

<h1>Orders</h1>
<table class="table">
    <thead>
        <tr>
            <th>@Html.DisplayNameFor(Model => Model.OrderDate
                )</th>
            <th>@Html.DisplayNameFor(Model => Model.ShipAddress)</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>@item.OrderDate</td>
            <td>@item.ShipAddress</td>
        </tr>
}
    </tbody>
</table>

<a asp-action="Index">Back</a>